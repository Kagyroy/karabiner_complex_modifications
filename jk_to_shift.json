{
  "title": "jk_to_right_shift",
  "rules": [
	{
	"description": "J,K同時押しでShift",
	"manipulators": [
	  {
		"type": "basic",
		"from": {
		  "simultaneous": [
			{"key_code": "j"},
			{"key_code": "k"}
		  ],
		  "simultaneous_options": {
			"detect_key_down_uninterruptedly": true
		  }
		},
		"to": [
		{"key_code": "right_shift"}
		]
	  }
	]
   }
 ]
}
