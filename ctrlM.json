{
    "title": "test3",
    "rules": [
{
  "description": "control-pageupをcontrol-shift-tabに変換",
  "manipulators": [
    { "type": "basic",
      "from": {
        "key_code": "page_up",
        "modifiers": { "mandatory": [ "control" ] }
      },
      "to": [
        {
          "key_code": "tab",
          "modifiers": [ "control", "shift" ]
        }
      ]
    }
  ]
}
]
}

